# Copy to .env, set SECRET. Do not commit .env.
#
# Required: 32 hex characters. Generate with: head -c 16 /dev/urandom | xxd -ps
SECRET=

# Optional: Tag from @MTProxybot after registering your proxy
PROXY_TAG=

# Number of worker processes (default: 1)
WORKERS=1

# Proxy port inside container (default: 443)
HOST_PORT=443

# Stats port for /stats endpoint (default: 8888)
STATS_PORT=8888

# Required when behind NAT: external IP clients connect to (e.g. Floating IP in OpenStack)
# Get it with: curl -s ifconfig.me
EXTERNAL_IP=
